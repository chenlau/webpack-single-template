{
  "name": "webpack-single-template",
  "version": "1.0.0",
  "description": "webpack单页项目模板",
  "main": "index.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "test": "rimraf dist && webpack --config build/webpack.config.test.js",
    "prod": "rimraf dist && webpack --config build/webpack.config.prod.js",
    "lint": "eslint --fix --ext .vue,.js src build",
    "lint-staged": "lint-staged",
    "build:stats": "webpack --config build/webpack.config.prod.js --json > stats.json",
    "analyz": "cross-env NODE_ENV=production ANALYZE=true npm_config_report=true npm run prod",
    "dll": "webpack --config build/webpack.config.dll"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{js, vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "caoyp",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "axios": "^0.18.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "happypack": "^5.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^1.6.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.2.1",
    "purgecss-webpack-plugin": "^4.0.3",
    "speed-measure-webpack-plugin": "^1.5.0",
    "terser-webpack-plugin": "^1.3.0",
    "thread-loader": "^3.0.4",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "connect-history-api-fallback": "^1.6.0",
    "css-loader": "^3.4.2",
    "eslint-plugin-vue": "^5.2.2",
    "express": "^4.17.1",
    "file-loader": "^5.1.0",
    "http-proxy-middleware": "^0.19.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "opn": "^6.0.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.6",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.1",
    "webpack-dashboard": "^3.0.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
